# Agentic Web Environment Variables

# Next.js
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Socket.IO Server
NEXT_PUBLIC_SOCKET_URL=http://localhost:3001

# Wallet Connect
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id

# OpenAI API (for AI agents)
OPENAI_API_KEY=your_openai_api_key

# Database (if using external DB)
DATABASE_URL=postgresql://username:password@localhost:5432/agentic_web

# Vector Database (Qdrant)
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_api_key

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
POLYGON_RPC_URL=https://polygon-mainnet.infura.io/v3/your_infura_key

# Fugio Protocol
FUGIO_CONTRACT_ADDRESS=0x...
FUGIO_PRIVATE_KEY=your_private_key_for_deployment

# AYD Domain System
AYD_REGISTRY_CONTRACT=0x...
AYD_RESOLVER_URL=https://ayd.resolver.com

# Memory System
MEMORY_RETENTION_DAYS=30
MEMORY_AUTO_CLEANUP=true
MEMORY_ENCRYPTION_KEY=your_encryption_key

# Agent Configuration
AGENT_MAX_TOKENS=4000
AGENT_TEMPERATURE=0.7
AGENT_MODEL=gpt-4

# Development
NODE_ENV=development
DEBUG=true
