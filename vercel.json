{
  "version": 2,
  "builds": [
    {
      "src": "demo/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "out"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/_next/static/(.*)", "dest": "/_next/static/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
